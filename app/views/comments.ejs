	<body>
		<div class="comments-form">
			<form  method="POST">
                Student Email: <input type="text" name="username" value="<%= username %>"  readonly="readonly"> 
                Course Code: <input type="text"  required>
				Course Name: <input type="text"  required>
                Course GPA: <input type="number"  required></br>  </br>
                Professor Rate from 0 to 10: <input type="number" required>   </br>           
                Your comments : <br /><textarea rows = "7" cols = "35" name = "comments" required></textarea>
                <input type="submit">
			</form>
		</div>
	</body>
</html>